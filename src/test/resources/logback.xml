        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="ca.jacob.cs6735.dt.Node" level="warn"></logger>
    <logger name="ca.jacob.cs6735.ensemble.RandomForestModel" level="info"></logger>
    <logger name="ca.jacob.cs6735.ensemble.RandomForest" level="info"></logger>
    <logger name="ca.jacob.cs6735.util.ML" level="info"></logger>
    <logger name="ca.jacob.cs6735.test" level="info"></logger>

    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>