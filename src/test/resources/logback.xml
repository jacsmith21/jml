<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="ca.jacob.jml.Model" level="warn"></logger>
    <logger name="ca.jacob.jml.KFold" level="info"></logger>

    <logger name="ca.jacob.jml.dt.Node" level="warn"></logger>

    <logger name="ca.jacob.jml.nb.NaiveBayesModel" level="info"></logger>
    <logger name="ca.jacob.jml.nb.NaiveBayes" level="info"></logger>
    <logger name="ca.jacob.jml.nb.ClassSummary" level="info"></logger>
    <logger name="ca.jacob.jml.nb.DiscreteAttribute" level="info"></logger>

    <logger name="ca.jacob.jml.ensemble.RandomForestModel" level="info"></logger>
    <logger name="ca.jacob.jml.ensemble.RandomForest" level="info"></logger>

    <logger name="ca.jacob.jml.neighbors.KNNModel" level="info"></logger>

    <logger name="ca.jacob.jml.util.ML" level="info"></logger>
    <logger name="ca.jacob.jml.util.Vector" level="info"></logger>
    <logger name="ca.jacob.cs6735.File" level="info"></logger>
    <logger name="ca.jacob.jml.util.DataSet" level="info"></logger>

    <logger name="ca.jacob.cs6735.test.MathTest" level="info"></logger>
    <logger name="ca.jacob.cs6735.test.AdaboostTest" level="debug"></logger>
    <logger name="ca.jacob.cs6735.test" level="info"></logger>
    <logger name="ca.jacob.cs6735.DataUtil" level="info"></logger>

    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>