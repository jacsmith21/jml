<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="ca.jacob.jml.dt.Node" level="warn"></logger>
    <logger name="ca.jacob.jml.util.Matrix" level="debug"></logger>

    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>